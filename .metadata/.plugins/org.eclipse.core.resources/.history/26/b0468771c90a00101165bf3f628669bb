package ash.com;

class Pro19 implements Runnable
{
	Pro19()
	{
		new Thread(this).start();
	}
	public void run()
	{
		for(int i=0;i<1000;i++)
		{
			System.out.println(i);
		}
	}
	public static void main(String[] args)
	{
		new Pro19();
		for(int i=1000;i<2000;i++)
		{
			System.out.println(i);
		}
	}
}

package ash.com;

public class Pro5 extends Thread
{
	static class A 
	{
		public void run()
		{
			for(int i=0;i<1000;i++)
			{
				System.out.println(i);
			}
		}
	}
	public static void main(String[] args)
	{
		A p1=new A();
		p1.start();
		for(int i=1000;i<2000;i++)
		{
			System.out.println(i);
		}
	}
}

package ash.com;

public class Pro18
{
	void startThread()
	{
		start();
	}
	public void run()
	{
		for(int i=0;i<1000;i++)
		{
			System.out.println(i);
		}
	}
	public static void main(String[] args)
	{
		Pro18 p1=new Pro18();
		p1.startThread();
		for(int i=0;i<2000;i++)
		{
			System.out.println(i);
		}
	}
}

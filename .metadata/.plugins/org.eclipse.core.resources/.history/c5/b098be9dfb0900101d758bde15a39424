package ash.com;

public class Pro8
{
	public static void main(String[] args)
	{
		class A extends Thread
		{
			public void run()
			{
				for(int i=0;i<1000;i++)
				{
					System.out.println(i);
				}
			}
		}
		A p1=new A();
		p1.start();
		for(int i=1000;i<2000;i++)
		{    System.out.println(i);
		}}
}

package ash.com;

class Pro17 extends Thread
{	
	Pro17()
	{
		start();
	}
	public static void main(String[] args)
	{
		Pro17 P1=new Pro17();
		
		for(int i=1000;i<2000;i++)
		{
			System.out.println(i);
		}						
	}
	public void run()
	{
		for(int i=0;i<1000;i++)
		{
			System.out.println(i);
		}
	}
}
	

package ash.com;

class Thread2 implements Runnable
{
	public void run()
	{
		for(int i=0;i<1000;i++)
		{
			System.out.println(i);
		}
	}
}
public class Pro3
{
	public static void main(String[] args)
	{
		Thread t=new Thread();
		Thread2 t1=new Thread2(t);}
		t1.start();
		for(int i=1000;i<2000;i++)
		{
			System.out.println(i);
		}
	}
}

package ash.com;

public class pro13
{
	public static void main(String[] args)
	{
		Runnable r=new Runnable();
		{
					public void run()
					{
						for(int i=0;i<1000;i++)
						{
							System.out.println(i);
						}
					}
				};
		}
		for(int i=1000;i<2000;i++)
		{
			System.out.println(i);
		}
	}
}
